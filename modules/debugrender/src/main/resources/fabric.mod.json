{
  "schemaVersion": 1,
  "id": "graphlib_debugrender",
  "version": "${version}",
  "name": "Graph Lib Debug Render",
  "description": "Client-side rendering of graphs for debugging server-side graphs.",
  "authors": [
    "Kneelawk"
  ],
  "contact": {
    "homepage": "https://github.com/Kneelawk/GraphLib",
    "issues": "https://github.com/Kneelawk/GraphLib/issues",
    "sources": "https://github.com/Kneelawk/GraphLib"
  },
  "license": "MIT",
  "icon": "assets/graphlib_debugrender/icon-128.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.kneelawk.graphlib.debugrender.impl.GraphLibDebugRenderFabricMod"
    ],
    "client": [
      "com.kneelawk.graphlib.debugrender.impl.client.GraphLibDebugRenderFabricModClient"
    ]
  },
  "mixins": [
    "graphlib_debugrender.mixins.json"
  ],
  "accessWidener": "graphlib_debugrender.accesswidener",
  "depends": {
    "graphlib": "*",
    "fabricloader": ">=0.14.0",
    "fabric-api": "*",
    "minecraft": ">=1.20.3 <1.21-",
    "java": ">=17"
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ],
      "parent": "graphlib"
    }
  }
}
