{
  "schemaVersion": 1,
  "id": "transfer_beams",
  "version": "${version}",
  "name": "Transfer Beams",
  "description": "A more complicated GraphLib example mod that beams that can transfer things between inventories.",
  "authors": [
    "Kneelawk"
  ],
  "contact": {
    "homepage": "https://github.com/Kneelawk/GraphLib",
    "issues": "https://github.com/Kneelawk/GraphLib/issues",
    "sources": "https://github.com/Kneelawk/GraphLib"
  },
  "license": "MIT",
  "icon": "assets/transfer_beams/icon-128.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "com.kneelawk.transferbeams.TransferBeamsMod"
      }
    ],
    "client": [
      {
        "value": "com.kneelawk.transferbeams.client.TransferBeamsModClient"
      }
    ],
    "fabric-datagen": [
      {
        "value": "com.kneelawk.transferbeams.datagen.TransferBeamsDatagen"
      }
    ]
  },
  "mixins": [
    "transfer_beams.mixins.json"
  ],
  "depends": {
    "graphlib": "*",
    "fabricloader": ">=0.14.0",
    "fabric-api": "*",
    "minecraft": ">=1.20.3 <1.21-",
    "java": ">=17"
  }
}
