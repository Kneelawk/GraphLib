{
  "schema_version": 1,
  "quilt_loader": {
    "group": "com.kneelawk",
    "id": "graphlib",
    "version": "${version}",
    "metadata": {
      "name": "Graph Lib",
      "description": "Library for helping mods that use graph networks, like Wired Redstone.",
      "contributors": {
        "Kneelawk": "Owner",
        "2xsaiko": "Original Author"
      },
      "contact": {
        "homepage": "https://github.com/Kneelawk/GraphLib",
        "issues": "https://github.com/Kneelawk/GraphLib/issues",
        "sources": "https://github.com/Kneelawk/GraphLib"
      },
      "license": "MIT"
    },
    "entrypoints": {
      "init": "com.kneelawk.graphlib.GraphLibMod"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.16.0-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=1.0.0-"
      },
      {
        "id": "minecraft",
        "versions": "1.18.x"
      },
      {
        "id": "java",
        "versions": ">=17"
      }
    ]
  },
  "mixin": "graphlib.mixins.json"
}
